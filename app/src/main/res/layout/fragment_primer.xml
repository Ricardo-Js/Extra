<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".BlankFragment" android:id="@+id/frameLayout"
                                                   android:background="#FFFFFF">
    <TextView
            android:text="+"
            android:layout_width="37dp"
            android:layout_height="40dp"
            android:id="@+id/textView2"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="184dp" android:textSize="36sp"
            android:layout_marginTop="10dp" app:layout_constraintTop_toBottomOf="@+id/etUno"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="190dp"/>
    <TextView
            android:text="SUMAR"
            android:layout_width="147dp"
            android:layout_height="77dp"
            android:id="@+id/textView3" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="68dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="220dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="144dp" android:textSize="36sp" app:layout_constraintHorizontal_bias="0.12"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etDos" android:layout_marginTop="7dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="100dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etUno" android:layout_marginTop="59dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="98dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="100dp"/>
    <Button
            android:text="RESULTADO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/etDos" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="152dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>